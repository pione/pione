
Rule OddSelector
  input '*.i'.all
Flow
  rule MakeResult
  rule OddSelector
  rule constraint ($I[1][1].i % 2) == 0
End

Rule MakeResult
  input '*.i'
  output ('{$*}.res'.touch).touch
End

Rule OddSelector
  output ('*.i'.remove).touch
End

Rule constraint ($I[1][1].i % 2) == 0
  input '*.i'
End
